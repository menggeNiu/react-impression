{
  "name": "react-impression",
  "version": "1.4.6",
  "author": "NewDadaFE",
  "description": "UI Components with React.",
  "sideEffects": false,
  "license": "MIT",
  "main": "dist/index.js",
  "module": "es/components/index.js",
  "scripts": {
    "start": "webpack-dev-server --config build/webpack.config.dev.js",
    "format": "prettier-eslint --write src/**/*.js && stylelint --syntax scss --fix src/**/*.scss",
    "clean": "gulp --gulpfile build/pre-deploy.js clean",
    "build": "yarn clean && yarn build:dist && gulp --gulpfile build/pre-deploy.js && yarn build:site",
    "build:site": "webpack -p --config build/webpack.config.site.js && gulp cp:dist:css --gulpfile build/pre-deploy.js",
    "build:dist": "webpack -p --config build/webpack.config.dist.js",
    "upload:site": "gulp sync:dev --gulpfile build/pre-deploy.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier-eslint --write",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --syntax scss --fix",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/NewDadaFE/react-impression.git"
  },
  "bugs": {
    "url": "https://github.com/NewDadaFE/react-impression/issues"
  },
  "keywords": [
    "impression",
    "react-impression",
    "react",
    "react-component",
    "component",
    "components",
    "ui",
    "framework",
    "frontend"
  ],
  "homepage": "https://github.com/NewDadaFE/react-impression",
  "dependencies": {
    "classnames": "^2.2.5",
    "perfect-scrollbar": "^1.3.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.3.2",
    "react-router": "3.0.5",
    "react-transition-group": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-css-modules": "^3.4.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-react-impression": "^2.0.2",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-clean": "^0.4.0",
    "gulp-rsync": "^0.0.8",
    "gulp-run-sequence": "^0.3.2",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.5",
    "marked": "^0.3.19",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-loader": "^2.1.4",
    "postcss-scss": "^1.0.5",
    "prettier-eslint-cli": "^4.7.1",
    "prismjs": "^1.14.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-scss": "^3.1.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  }
}
